{
  "name": "mossengine/jcode",
  "type": "library",
  "description": "PHP Class to enable JSON driven programmatic instructions to execute controlled php code in the backend.",
  "keywords": [
    "backend",
    "json",
    "programmatic",
    "code",
    "execute"
  ],
  "homepage": "https://github.com/Mossengine/JCode",
  "support": {
    "issues": "https://github.com/Mossengine/JCode/issues",
    "source": "https://github.com/Mossengine/JCode"
  },
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Brendon Moss",
      "email": "brenmoss@mossengine.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ramsey/uuid": "3.4.1",
    "illuminate/support": "5.2.32"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2",
    "phpunit/phpunit": "~4.0 || ~5.0"
  },
  "autoload": {
    "psr-4": {
      "Mossengine\\JCode\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs"
    ],
    "phpunit": "phpunit",
    "phpcs": "php-cs-fixer fix -v --diff --dry-run"
  }
}
